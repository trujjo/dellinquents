{
    "name": "dell-med-underground",
    "version": "1.0.0",
    "description": "A static website with password protection for Dell Medical School students.",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "build": "npm run clean && npm run copy-static",
        "clean": "rm -rf dist",
        "copy-static": "cp -r public dist"
    },
